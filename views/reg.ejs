<% include header %>
<form class="form-horizontal" method="post">
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-3 control-label col-md-2 col-md-offset-2"
               style="line-height: 30px;">用户名</label>
        <div class="col-sm-5" style="margin-bottom: 30px;">
            <input type="text" class="form-control" id="inputEmail3" placeholder="Username" name="username">
        </div>
    </div>
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-3 control-label col-md-2 col-md-offset-2"
               style="line-height: 30px;">姓名</label>
        <div class="col-sm-5" style="margin-bottom: 30px;">
            <input type="text" class="form-control" id="inputEmail3" placeholder="Really Name" name="reallyname">
        </div>
    </div>
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-3 control-label col-md-2 col-md-offset-2"
               style="line-height: 30px;">密码</label>
        <div class="col-sm-5" style="margin-bottom: 30px;">
            <input type="password" class="form-control" id="inputEmail3" placeholder="Password" name="password">
        </div>
    </div>
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-3 control-label col-md-2 col-md-offset-2"
               style="line-height: 30px;">确认密码</label>
        <div class="col-sm-5" style="margin-bottom: 30px;">
            <input type="password" class="form-control" id="inputEmail3" placeholder="Password" name="password_repeat">
        </div>
    </div>
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-3 control-label col-md-2 col-md-offset-2"
               style="line-height: 30px;">手机</label>
        <div class="col-sm-5" style="margin-bottom: 30px;">
            <input type="text" class="form-control" id="inputEmail3" placeholder="Phone Namber" name="phone">
        </div>
    </div>
    <div class="form-group">
        <label for="inputEmail3" class="col-sm-3 control-label col-md-2 col-md-offset-2"
               style="line-height: 30px;">邮箱</label>
        <div class="col-sm-5" style="margin-bottom: 30px;">
            <input type="email" class="form-control" id="inputEmail3" placeholder="Email" name="email">
        </div>
    </div>
    <button type="submit" class="btn btn-success"style="position: relative;left:360px;">提交</button>
    <button type="submit" class="btn btn-danger"style="position: relative;left:390px" >取消</button>
</form>
<% include footer %>